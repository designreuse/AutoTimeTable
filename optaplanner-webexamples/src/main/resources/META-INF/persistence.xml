<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
    <persistence-unit name="optaplanner-web-pu" transaction-type="JTA">
        <jta-data-source>jdbc/dersProgram</jta-data-source>
        <class>ororg.optaplanner.examples.common.domain.AbstractPersistable</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.Course</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.CourseSchedule</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.Curriculum</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.Day</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.Lecture</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.Period</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.Room</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.Teacher</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.Timeslot</class>
        <class>org.optaplanner.examples.curriculumcourse.domain.UnavailablePeriodPenalty</class>
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
        <properties>
            <property name="javax.persistence.schema-generation.database.action" value="create"/>
        </properties>
    </persistence-unit>
</persistence>
